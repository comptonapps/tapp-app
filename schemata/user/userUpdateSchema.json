{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "title": "The root schema",
  "description": "The root schema comprises the entire JSON document.",
  "default": {},
  "examples": [
    {
      "password": "password",
      "email": "j@c.com",
      "first_name": "Jeff",
      "last_name": "Jones",
      "city": "Seattle",
      "state": "WA"
    }
  ],
  "required": [],
  "properties": {
    "password": {
      "$id": "#/properties/password",
      "type": "string",
      "title": "The password schema",
      "minLength": 8,
      "description": "An explanation about the purpose of this instance.",
      "default": "",
      "examples": ["password"]
    },
    "email": {
      "$id": "#/properties/email",
      "type": "string",
      "title": "The email schema",
      "format": "email",
      "minLength": 6,
      "maxLength": 50,
      "description": "An explanation about the purpose of this instance.",
      "default": "",
      "examples": ["j@c.com"]
    },
    "first_name": {
      "$id": "#/properties/first_name",
      "type": "string",
      "title": "The first_name schema",
      "minLength": 1,
      "maxLength": 15,
      "description": "string representing user's first name",
      "default": "",
      "examples": ["Jeff"]
    },
    "last_name": {
      "$id": "#/properties/last_name",
      "type": "string",
      "title": "The last_name schema",
      "minLength": 1,
      "maxLength": 20,
      "description": "string representing user's last name",
      "default": "",
      "examples": ["Jones"]
    },
    "city": {
      "$id": "#/properties/city",
      "type": "string",
      "title": "The city schema",
      "minLength": 2,
      "maxLength": 48,
      "description": "An explanation about the purpose of this instance.",
      "default": "",
      "examples": ["Seattle"]
    },
    "state": {
      "$id": "#/properties/state",
      "type": "string",
      "title": "The state schema",
      "minLength": 2,
      "maxLength": 2,
      "description": "An explanation about the purpose of this instance.",
      "default": "",
      "examples": ["WA"]
    }
  },
  "additionalProperties": false
}
